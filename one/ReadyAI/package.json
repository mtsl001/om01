{
  "name": "readyai-core",
  "displayName": "ReadyAI - Personal AI Development Orchestrator",
  "version": "1.0.0",
  "description": "ReadyAI - Personal AI Development Orchestrator that transforms project ideas into production-ready code through intelligent context management and dependency-aware generation",
  "publisher": "readyai",
  "icon": "assets/icons/readyai-icon.png",
  "engines": {
    "vscode": "^1.84.0",
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "categories": [
    "AI",
    "Programming Languages",
    "Education",
    "Snippets",
    "Testing",
    "Other"
  ],
  "keywords": [
    "ai",
    "development",
    "orchestrator", 
    "code-generation",
    "context-management",
    "vscode-extension",
    "typescript",
    "sqlite",
    "vector-database",
    "milvus",
    "api-gateway",
    "authentication",
    "error-handling",
    "express",
    "jwt",
    "rag-system",
    "claude-sonnet",
    "readyai"
  ],
  "license": "MIT",
  "author": {
    "name": "ReadyAI Team",
    "email": "contact@readyai.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtsl001/om01.git",
    "directory": "one/ReadyAI"
  },
  "bugs": {
    "url": "https://github.com/mtsl001/om01/issues"
  },
  "homepage": "https://github.com/mtsl001/om01/tree/main/one/ReadyAI",
  "main": "dist/extension.js",
  "types": "dist/index.d.ts",
  "bin": {
    "readyai": "bin/readyai.js"
  },
  "activationEvents": [
    "onLanguage",
    "onStartupFinished",
    "workspaceContains:**/.readyai-config.json"
  ],
  "contributes": {
    "views": {
      "readyai-ActivityBar": [
        {
          "type": "webview",
          "id": "readyai.SidebarProvider",
          "name": "ReadyAI Assistant",
          "icon": "assets/icons/readyai-icon.svg"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "readyai-ActivityBar",
          "title": "ReadyAI",
          "icon": "assets/icons/readyai-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "readyai.createProject",
        "title": "Create New Project",
        "icon": "$(add)",
        "category": "ReadyAI"
      },
      {
        "command": "readyai.contextSearch",
        "title": "Search Context",
        "icon": "$(search)",
        "category": "ReadyAI"
      },
      {
        "command": "readyai.generateCode",
        "title": "Generate Code",
        "icon": "$(code)",
        "category": "ReadyAI"
      },
      {
        "command": "readyai.openSettings",
        "title": "Settings",
        "icon": "$(settings-gear)",
        "category": "ReadyAI"
      },
      {
        "command": "readyai.viewHistory",
        "title": "View History",
        "icon": "$(history)",
        "category": "ReadyAI"
      }
    ],
    "configuration": {
      "title": "ReadyAI Configuration",
      "properties": {
        "readyai.openrouter.apiKey": {
          "type": "string",
          "description": "OpenRouter API Key for Claude Sonnet 4 access",
          "scope": "application"
        },
        "readyai.context.maxTokens": {
          "type": "number",
          "default": 100000,
          "description": "Maximum tokens for context management"
        },
        "readyai.generation.autoApprove": {
          "type": "boolean",
          "default": false,
          "description": "Automatically approve low-risk code generation"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build": "tsc --build && npm run build:ui && npm run build:webview",
    "build:ui": "cd packages/ui && npm run build",
    "build:webview": "cd apps/vscode/webview-ui && npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "build:dev": "NODE_ENV=development npm run build",
    "compile": "npm run check-types && npm run lint && node esbuild.mjs",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.mjs --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run build:webview && npm run lint && node esbuild.mjs --production",
    "check-types": "tsc --noEmit && cd apps/vscode/webview-ui && tsc -b --noEmit",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:ui\" \"npm run dev:extension\"",
    "dev:api": "nodemon --exec \"tsx apps/api/server.ts\" --watch apps/api --watch packages",
    "dev:ui": "cd packages/ui && npm run dev", 
    "dev:extension": "cd apps/vscode && npm run compile:watch",
    "dev:webview": "cd apps/vscode/webview-ui && npm run dev",
    "start": "node dist/apps/api/server.js",
    "start:prod": "NODE_ENV=production npm start",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js && playwright test",
    "test:auth": "jest packages/auth",
    "test:gateway": "jest packages/api-gateway",
    "test:error": "jest packages/error-handling",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage .nyc_output apps/vscode/webview-ui/build",
    "clean:deps": "rimraf node_modules packages/*/node_modules apps/*/node_modules",
    "prepare": "husky install",
    "prepack": "npm run build:prod",
    "postinstall": "npm run setup:database && npm run setup:vector-db && npm run setup:auth && npm run install:webview",
    "install:webview": "cd apps/vscode/webview-ui && npm install",
    "setup:database": "node scripts/setup-database.js",
    "setup:vector-db": "node scripts/setup-vector-db.js",
    "setup:auth": "node scripts/setup-auth.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:seed": "node scripts/seed-database.js",
    "db:backup": "node scripts/backup-database.js",
    "vector:init": "node scripts/init-vector-collections.js",
    "vector:health": "node scripts/check-vector-health.js",
    "api:health": "curl -f http://localhost:3000/api/health || exit 1",
    "auth:test": "node scripts/test-auth.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.37.0",
    "@milvus-io/milvus-lite": "^2.4.0",
    "@sentencepiece/sentencepiece": "^0.11.0",
    "better-sqlite3": "^12.2.0",
    "sqlite3": "^5.1.7",
    "sentence-transformers": "^1.1.0",
    "@tensorflow/tfjs": "^4.21.0",
    "@tensorflow/tfjs-node": "^4.21.0",
    "numpy": "^0.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0",
    "axios": "^1.8.2",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "uuid": "^11.1.0",
    "vscode-uri": "^3.1.0",
    "zod": "^3.24.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "async": "^3.2.6",
    "p-queue": "^8.0.1",
    "p-limit": "^6.1.0",
    "p-retry": "^6.2.1",
    "p-timeout": "^6.1.4",
    "rxjs": "^7.8.1",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "inversify": "^6.0.2",
    "glob": "^11.0.0",
    "fast-glob": "^3.3.2",
    "chokidar": "^4.0.1",
    "mime-types": "^2.1.35",
    "semver": "^7.6.3",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "inquirer": "^12.0.0",
    "commander": "^12.1.0",
    "yargs": "^17.7.2",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "connect-flash": "^0.1.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "http-proxy-middleware": "^3.0.3",
    "node-cache": "^5.1.2",
    "redis": "^4.7.0",
    "ioredis": "^5.4.1",
    "http-status-codes": "^2.3.0",
    "validator": "^13.12.0",
    "sanitize-html": "^2.13.1",
    "rate-limiter-flexible": "^5.0.3",
    "fast-deep-equal": "^3.1.3",
    "execa": "^9.5.2",
    "globby": "^14.0.2",
    "ignore": "^7.0.3",
    "tree-sitter-wasms": "^0.1.11",
    "web-tree-sitter": "^0.22.6",
    "serialize-error": "^11.0.3",
    "simple-git": "^3.27.0",
    "strip-ansi": "^7.1.0",
    "posthog-node": "^5.8.0",
    "ulid": "^2.4.0",
    "reconnecting-eventsource": "^1.6.4",
    "jwt-decode": "^4.0.0",
    "open": "^10.1.2",
    "mammoth": "^1.8.0",
    "pdf-parse": "^1.1.1",
    "cheerio": "^1.0.0",
    "turndown": "^7.2.0",
    "image-size": "^2.0.2",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.4",
    "isbinaryfile": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/vscode": "^1.84.0",
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/uuid": "^10.0.0",
    "@types/lodash": "^4.17.13",
    "@types/async": "^3.2.24",
    "@types/mime-types": "^2.1.4",
    "@types/semver": "^7.5.8",
    "@types/better-sqlite3": "^7.6.11",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express-session": "^1.18.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/connect-flash": "^0.0.40",
    "@types/multer": "^1.4.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.12.2",
    "@types/sanitize-html": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^3.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "tsx": "^4.19.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "supertest": "^7.0.0",
    "nodemon": "^3.1.7",
    "concurrently": "^9.1.0",
    "rimraf": "^6.0.1",
