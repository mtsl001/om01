  },
  "optionalDependencies": {
    "@milvus-io/milvus-sdk-node": "^0.5.0",
    "faiss-node": "^0.5.1",
    "hnswlib-node": "^3.0.0"
  },
  "peerDependencies": {
    "vscode": "^1.95.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/packages",
      "<rootDir>/apps"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/__tests__/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "packages/**/*.ts",
      "apps/**/*.ts",
      "!**/*.d.ts",
      "!**/__tests__/**",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapping": {
      "^@readyai/(.*)$": "<rootDir>/packages/$1/src",
      "^@/(.*)$": "<rootDir>/apps/$1/src"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "packages/**/*.ts",
      "apps/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/dist/**"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/AI-Powered-blue",
      "href": "https://readyai.dev",
      "description": "AI-Powered Development"
    }
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "qna": "https://github.com/mtsl001/om01/discussions",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "assets/icons"
  ]
}
